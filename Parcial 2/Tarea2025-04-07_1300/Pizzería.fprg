                    <input variable="ingrediente"/>
                    <assign variable="pizza" expression="&quot;Pizza no vegetariana con mozarella, tomate y&quot; + ingrediente"/>
                </else>
            </if>
            <output expression="&quot;Pizza&quot;" newline="True"/>
        </body>
