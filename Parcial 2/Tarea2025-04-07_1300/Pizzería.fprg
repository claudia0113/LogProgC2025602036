    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="tipo" type="String" array="False" size=""/>
            <declare name="ingrediente" type="String" array="False" size=""/>
            <declare name="pizza" type="String" array="False" size=""/>
            <output expression="&quot;&#191;Quieres pizza vegetariana? (si/no):&quot;" newline="True"/>
            <input variable="tipo"/>
            <if expression="tipo==&quot;si/no&quot;">
                <then>
                    <output expression="&quot;Ingredientes: Pimineto o tofu&quot;" newline="True"/>
