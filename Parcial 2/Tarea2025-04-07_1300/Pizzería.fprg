                    <assign variable="pizza" expression="&quot;Pizza vegetariana con mozzarella, tomate y &quot; + ingrediente"/>
                </then>
                <else>
                    <output expression="&quot;Ingredientes: Peperoni, jam&#243;n o salm&#243;n&quot;" newline="True"/>
